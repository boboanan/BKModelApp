CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FSCalendar" "${PODS_ROOT}/Headers/Public/KRLCollectionViewGridLayout" "${PODS_ROOT}/Headers/Public/SSLunarDate" "${PODS_ROOT}/Headers/Public/XHRealTimeBlur" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FSCalendar" -isystem "${PODS_ROOT}/Headers/Public/KRLCollectionViewGridLayout" -isystem "${PODS_ROOT}/Headers/Public/SSLunarDate" -isystem "${PODS_ROOT}/Headers/Public/XHRealTimeBlur" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-FSCalendar" -l"Pods-KRLCollectionViewGridLayout" -l"Pods-SSLunarDate" -l"Pods-XHRealTimeBlur" -l"Pods-pop" -l"c++" -framework "CoreGraphics" -framework "Foundation" -framework "QuartzCore" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods